{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "/", "destination": "/api" },
    { "source": "/python/:path*", "destination": "/api/python/:path*" }
    ,{ "source": "/c/:path*", "destination": "/api/c/:path*" }
  ],
  "redirects": [
    {
      "source": "/web/:path*",
      "destination": "https://raw.githubusercontent.com/milan-m-antony/code-vault/main/web/:path*",
      "permanent": false
    }
    ,
    {
      "source": "/python/:path*",
      "destination": "https://raw.githubusercontent.com/milan-m-antony/code-vault/main/python/:path*",
      "permanent": false
    }
    ,
    {
      "source": "/c/:path*",
      "destination": "https://raw.githubusercontent.com/milan-m-antony/code-vault/main/c/:path*",
      "permanent": false
    }
  ]
}
